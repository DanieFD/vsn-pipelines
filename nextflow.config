
profiles {

    standard {
        process.executor = 'local'
    }
    qsub {
        process.executor = 'pbs'
    }
    docker {
        docker.enabled = true
    }
    singularity {
        singularity.enabled = true
        singularity.autoMounts = true
    }

    // tool-specific profiles:
    scenic {
        includeConfig 'src/scenic/nextflow.config'
    }

}

