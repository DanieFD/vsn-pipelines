
params {
    sc {
        scenic {
            aucell {
                output = 'aucell_output.loom'
                // motif enrichment arguments:
                rank_threshold = 5000
                auc_threshold = 0.05
                nes_threshold = 3.0
                // Resources settings
                maxForks = 1
                numWorkers = 2
                // Following parameters are not used except params.sc.scenic.labels.processExecutor = 'qsub'
                pmem = '2gb' // Not used except params.sc.scenic.labels.processExecutor = 'qsub'
                walltime = '24:00:00' // Not used except params.sc.scenic.labels.processExecutor = 'qsub'
            }
        }
    }
}